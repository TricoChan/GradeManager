<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" version="2.5">
  <welcome-file-list>
    <welcome-file>index.html</welcome-file>
  </welcome-file-list>
  <servlet>
    <servlet-name>note-insert</servlet-name>
    <servlet-class>cn.trico.servlet.SubmitNoteServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>note-insert</servlet-name>
    <url-pattern>/note-insert</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>grade-query</servlet-name>
    <servlet-class>cn.trico.servlet.GradeQueryServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>grade-query</servlet-name>
    <url-pattern>/grade-query</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>note-all</servlet-name>
    <servlet-class>cn.trico.servlet.NoteAllServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>note-all</servlet-name>
    <url-pattern>/note-all</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>my-note</servlet-name>
    <servlet-class>cn.trico.servlet.MyNoteServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>my-note</servlet-name>
    <url-pattern>/my-note</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>work-all</servlet-name>
    <servlet-class>cn.trico.servlet.WorkAllServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>work-all</servlet-name>
    <url-pattern>/work-all</url-pattern>
  </servlet-mapping>
  <servlet>
    <display-name>upload</display-name>
    <servlet-name>upload</servlet-name>
    <servlet-class>cn.trico.servlet.UploadServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>upload</servlet-name>
    <url-pattern>/upload</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>pre-fetch</servlet-name>
    <servlet-class>cn.trico.servlet.PreFetchServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>pre-fetch</servlet-name>
    <url-pattern>/pre-fetch</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>login</servlet-name>
    <servlet-class>cn.trico.servlet.LoginServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>login</servlet-name>
    <url-pattern>/login</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>regist</servlet-name>
    <servlet-class>cn.trico.servlet.RegisterServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>regist</servlet-name>
    <url-pattern>/regist</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>session</servlet-name>
    <servlet-class>cn.trico.servlet.GetSessionServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>session</servlet-name>
    <url-pattern>/session</url-pattern>
  </servlet-mapping>
</web-app>